{
  "comments": {
    "lineComment": "'",
    "blockComment": ["REM", ""]
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    {"open": "(", "close": ")"},
    {"open": "[", "close": "]"},
    {"open": "\"", "close": "\""}
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(SUB|FUNCTION|DO|FOR|WHILE|IF)",
      "end": "^\\s*(END SUB|END FUNCTION|LOOP|NEXT|WEND|END IF)"
    }
  }
}
